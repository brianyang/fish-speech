{
  "version": 2,
  "builds": [
    {
      "src": "tools/api.py",
      "use": "@vercel/python"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/tools/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/next.config.js"
    }
  ]
}
